"hasDefaulted"=abs(round(rnorm(100,0,1),0)),
"line"=round(rnorm(100,5,1),0))
# Ordering the dataset
df <- df[order(df$Score),]
# Creating the cumulative density
df$cumden <- (cumsum(df$hasDefaulted)^(0.5)/sum(df$hasDefaulted))*9
df$line_ <- (cumsum(df$line)/sum(df$line))
# Creating the % of population
df$perpop <- (seq(nrow(df))/nrow(df))
# Ploting
plot(df$perpop,df$cumden,type="l",xlab="% Debitori Totali",ylab="% Debitori in Default")
lines(df$perpop,cumsum(df$line)/sum(df$line), col = "orange")
